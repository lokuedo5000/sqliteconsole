<body>
    <div class="menu-sqlite">
        <div class="action-sqlite">
            <div class="btn-action icon-file-empty modal-trigger tooltipped" data-position="right"
                data-tooltip="Crear base de datos" href="#newdb"></div>
            <div class="btn-action icon-library_add open-db-action tooltipped" data-position="right"
                data-tooltip="Agregar base de datos"></div>
            <div class="btn-action icon-remove_circle_outline tooltipped close-db" data-position="right"
                data-tooltip="Remover"></div>
            <div class="btn-action icon-info_outline tooltipped open-console" data-position="right" data-tooltip="Console">
            </div>
            <div class="btn-action icon-play1 run-code tooltipped" data-position="right" data-tooltip="Ejecutar cÃ³digo">
            </div>
        </div>
        <div class="list-db" id="list-db">
            <!-- <label>
                <span class="icon-database2"></span>
                <div class="db-text">Articles</div>
                <input name="group1" type="radio" />
                <span class="radios-input"></span>
            </label> -->
        </div>
    </div>
    <div class="console-body">
        <textarea id="sql-editor" spellcheck="false" class="full-height-editor"></textarea>
    </div>

    <div class="console-result">
        <textarea id="text-editor"></textarea>
    </div>

    <!-- nueva base de datos -->
    <div id="newdb" class="modal">
        <div class="modal-content">
            <h4>Nueva base de datos</h4>
            <div class="row">
                <form class="col s12">
                    <div class="row">
                        <div class="col s6">
                            <label>Nombre</label>
                            <input placeholder="Nombre" name="name_db" type="text" class="validate">
                        </div>
                        <div class="col s6">
                            <label>Extension</label>
                            <select class="browser-default" name="extension_db">
                                <option value="" disabled>Extension</option>
                                <option value=".db" selected>db</option>
                                <option value=".sqlite">sqlite</option>
                            </select>
                        </div>
                    </div>
                    <!-- <div class="row">
                        <div class="input-field col s12">
                            <input id="password" type="password" name="password_db" class="validate">
                            <label for="password">Password</label>
                        </div>
                    </div> -->


                </form>
            </div>
        </div>
        <div class="modal-footer">
            <a href="#!" class="waves-effect waves-green btn-flat create-db">Crear</a>
        </div>
    </div>

    <!-- abrir base de datos -->
    <div id="opendb" class="modal">
        <div class="modal-content">
            <h4>Nueva base de datos</h4>
            <div class="row m0">
                <form class="col s12">

                    <div class="row m0">
                        <div class="input-field col s12">
                            <input id="password" type="password" name="password_db" class="validate">
                            <label for="password">Password</label>
                        </div>
                    </div>


                </form>
            </div>
        </div>
        <div class="modal-footer">
            <a href="#!" class="waves-effect waves-green btn-flat open-db">Crear</a>
        </div>
    </div>


